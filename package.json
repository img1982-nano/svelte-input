{
  "name": "@jill64/svelte-input",
  "version": "1.5.2",
  "description": "➡️ Functional Input Component Set for Svelte",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-input/issues",
  "homepage": "https://github.com/jill64/svelte-input#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-input.git"
  },
  "keywords": [
    "components",
    "input",
    "suite",
    "svelte"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "test": "playwright test",
    "check": "svelte-kit sync && npx svelte-check",
    "format": "npx @jill64/psx",
    "lint": "npx eslint . && npm run check"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/sentry-sveltekit-cloudflare": "1.5.0",
    "@jill64/svelte-suite": "0.0.12",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/svelte-tailwind": "0.0.7",
    "@jill64/playwright-config": "1.0.0",
    "@jill64/eslint-config-svelte": "1.0.0",
    "svelte-highlight-switcher": "0.0.1",
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-cloudflare": "2.3.4",
    "@sveltejs/kit": "1.27.7",
    "nanoid": "5.0.4",
    "svelte": "4.2.8",
    "svelte-highlight": "7.4.2",
    "svelte-mq-store": "1.0.0",
    "typescript": "5.3.2",
    "vite": "5.0.5"
  },
  "dependencies": {
    "@jill64/async-observer": "1.2.0",
    "bignumber.js": "9.1.2",
    "svelte-loading-spinners": "0.3.4"
  }
}
